{
	"name": "nextjs-tempalte",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "concurrently --kill-others \"npm run start:api\" \"next dev\"",
		"build": "next build",
		"start": "next start",
		"start:api": "ts-node src/mock/index.mjs",
		"lint": "next lint",
		"format": "prettier --no-editorconfig --write ./src*",
		"format:check": "prettier --no-editorconfig --check ./src*",
		"format:current": "prettier --no-editorconfig --write $(git diff --name-only develop)",
		"prepare": "husky",
		"lint-staged": "lint-staged",
		"test": "jest --watch",
		"test:ci": "jest --ci"
	},
	"dependencies": {
		"@emotion/cache": "^11.13.1",
		"@emotion/react": "^11.13.0",
		"@emotion/styled": "^11.13.0",
		"@mui/lab": "^5.0.0-alpha.173",
		"@mui/material": "^5.16.6",
		"@mui/material-nextjs": "^5.16.6",
		"@types/lodash-es": "^4.17.12",
		"localforage": "^1.10.0",
		"lodash-es": "^4.17.21",
		"next": "14.2.5",
		"pino": "^9.3.2",
		"react": "^18",
		"react-dom": "^18",
		"react-intl": "^6.6.8",
		"recoil": "^0.7.7"
	},
	"devDependencies": {
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.5.0",
		"@testing-library/react": "^16.0.0",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"concurrently": "^8.2.2",
		"cors": "^2.8.5",
		"eslint": "^8",
		"eslint-config-next": "14.2.5",
		"eslint-plugin-unused-imports": "^4.1.3",
		"express": "^4.19.2",
		"husky": "^9.1.5",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"lint-staged": "^15.2.9",
		"prettier": "^3.3.3",
		"ts-node": "^10.9.2",
		"typescript": "^5"
	}
}
